# Jarvis Voice Assistant - Environment Variables

# OpenAI API
OPENAI_API_KEY=your_openai_api_key_here

# GitHub Integration
GITHUB_TOKEN=your_github_token_here
GITHUB_DEFAULT_REPO=owner/repo

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=true

# Session Configuration
SESSION_EXPIRY_MINUTES=30

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=60
RATE_LIMIT_REQUESTS_PER_SESSION=100

# Audio Configuration
TTS_VOICE=alloy
TTS_MODEL=tts-1
STT_MODEL=whisper-1
LLM_MODEL=gpt-4

# Data Refresh
API_REFRESH_INTERVAL_SECONDS=180

